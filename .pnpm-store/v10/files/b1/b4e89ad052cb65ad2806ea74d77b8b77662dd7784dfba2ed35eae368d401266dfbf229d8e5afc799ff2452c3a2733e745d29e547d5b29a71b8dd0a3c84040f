bject.entries(mediaConfig)\n            .reduce<string[]>((acc, [media, value]) => {\n                if (value) {\n                    acc.push(`${media}=${value}`);\n                }\n                return acc;\n            }, [])\n            .join(' ');\n        title = `${title} [${mediaPart}]`;\n    }\n\n    return title;\n};\n\nexport const ColPresenter = ({size, ...props}: ColProps) => (\n    <Col size={size} {...props}>\n        <Box style={{height: '100%'}}>\n            <Text variant=\"code-1\" color=\"complementary\">\n                {renderTitle(size)}\n            </Text>\n        </Box>\n    </Col>\n);\n"]}