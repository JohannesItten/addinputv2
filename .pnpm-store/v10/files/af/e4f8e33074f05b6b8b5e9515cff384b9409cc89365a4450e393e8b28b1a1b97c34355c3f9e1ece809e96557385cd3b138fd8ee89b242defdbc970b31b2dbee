$1')\n            .replace(/(\\s)+>/g, '>');\n    });\n}\n\nexport function getStringViewBox(data: SVGIconStringData) {\n    const match = data.match(/viewBox=([\"']?)([\\d\\s,-]+)\\1/);\n\n    return match ? match[2] : undefined;\n}\n"]}