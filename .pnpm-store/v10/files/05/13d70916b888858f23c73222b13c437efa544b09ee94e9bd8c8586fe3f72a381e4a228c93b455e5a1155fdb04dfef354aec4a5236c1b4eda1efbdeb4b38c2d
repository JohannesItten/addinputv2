import { jsx as _jsx } from "react/jsx-runtime";
import { block } from "../utils/cn.js";
import "./Skeleton.css";
const b = block('skeleton');
export function Skeleton({ className, style, qa, animation = 'gradient' }) {
    return (_jsx("div", { className: b({
            animation,
        }, className), style: style, "data-qa": qa }));
}
