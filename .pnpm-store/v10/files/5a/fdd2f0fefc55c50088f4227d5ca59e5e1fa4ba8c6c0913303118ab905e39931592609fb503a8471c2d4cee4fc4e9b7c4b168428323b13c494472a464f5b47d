import { jsx as _jsx } from "react/jsx-runtime";
import { Text } from "../Text/index.js";
import { bAlert } from "./constants.js";
export const AlertTitle = ({ text, className }) => {
    return (_jsx(Text, { variant: "subheader-2", className: bAlert('title', className), children: text }));
};
