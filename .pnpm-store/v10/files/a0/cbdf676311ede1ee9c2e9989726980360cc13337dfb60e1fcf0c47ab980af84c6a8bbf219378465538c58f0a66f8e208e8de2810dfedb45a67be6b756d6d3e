{"version":3,"file":"filterDOMProps.js","sourceRoot":"../../../../src","sources":["components/utils/filterDOMProps.ts"],"names":[],"mappings":"AAEA,MAAM,YAAY,GAAG,IAAI,GAAG,CAAC,CAAC,IAAI,CAAC,CAAC,CAAC;AAErC,MAAM,kBAAkB,GAAG,IAAI,GAAG,CAAC;IAC/B,YAAY;IACZ,iBAAiB;IACjB,kBAAkB;IAClB,cAAc;CACjB,CAAC,CAAC;AAaH,MAAM,MAAM,GAAG,aAAa,CAAC;AAE7B,MAAM,UAAU,cAAc,CAC1B,KAAwC,EACxC,UAAmB,EAAE;IAErB,MAAM,EAAC,SAAS,EAAE,SAAS,EAAC,GAAG,OAAO,CAAC;IACvC,MAAM,aAAa,GAAG,EAAE,CAAC;IAEzB,KAAK,MAAM,IAAI,IAAI,KAAK,EAAE,CAAC;QACvB,IACI,MAAM,CAAC,SAAS,CAAC,cAAc,CAAC,IAAI,CAAC,KAAK,EAAE,IAAI,CAAC;YACjD,CAAC,YAAY,CAAC,GAAG,CAAC,IAAI,CAAC;gBACnB,CAAC,SAAS,IAAI,kBAAkB,CAAC,GAAG,CAAC,IAAI,CAAC,CAAC;gBAC3C,SAAS,EAAE,GAAG,CAAC,IAAI,CAAC;gBACpB,MAAM,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC,EACxB,CAAC;YACC,mBAAmB;YACnB,aAAa,CAAC,IAAI,CAAC,GAAG,KAAK,CAAC,IAAI,CAAC,CAAC;QACtC,CAAC;IACL,CAAC;IAED,OAAO,aAAa,CAAC;AACzB,CAAC","sourcesContent":["import type {AriaLabelingProps} from '../types';\n\nconst DOMPropNames = new Set(['id']);\n\nconst labelablePropNames = new Set([\n    'aria-label',\n    'aria-labelledby',\n    'aria-describedby',\n    'aria-details',\n]);\n\ninterface Options {\n    /**\n     * If labelling associated aria properties should be included in the filter.\n     */\n    labelable?: boolean;\n    /**\n     * A Set of other property names that should be included in the filter.\n     */\n    propNames?: Set<string>;\n}\n\nconst propRe = /^(data-.*)$/;\n\nexport function filterDOMProps(\n    props: {id?: string} & AriaLabelingProps,\n    options: Options = {},\n): {id?: string} & AriaLabelingProps {\n    const {labelable, propNames} = options;\n    const filteredProps = {};\n\n    for (const prop in props) {\n        if (\n            Object.prototype.hasOwnProperty.call(props, prop) &&\n            (DOMPropNames.has(prop) ||\n                (labelable && labelablePropNames.has(prop)) ||\n                propNames?.has(prop) ||\n                propRe.test(prop))\n        ) {\n            // @ts-expect-error\n            filteredProps[prop] = props[prop];\n        }\n    }\n\n    return filteredProps;\n}\n"]}